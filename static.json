{
  "root": "build/",
  "routes": {
    "/**": "index.html"
  },
  "https_only": true,
  "headers": {
    "/**": {
      "Strict-Transport-Security": "max-age=31557600",
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  }
}
